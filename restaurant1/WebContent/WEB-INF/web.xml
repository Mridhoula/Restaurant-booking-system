<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee" 
         version="2.5" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"> 
<welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
</welcome-file-list>
<context-param> 
   <description>Location to store uploaded file</description> 
   <param-name>file-upload</param-name> 
   <param-value>
      c:\apache-tomcat-7.0.47\webapps\
   </param-value> 
</context-param>

<servlet>
<servlet-name>hello</servlet-name>
<servlet-class>restauranttiming.resregister</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>hello</servlet-name>
<url-pattern>/hello1</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>LOGIN</servlet-name>
<servlet-class>restauranttiming.reslogin</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>LOGIN</servlet-name>
<url-pattern>/login</url-pattern></servlet-mapping>

<servlet>
<servlet-name>menu</servlet-name>
<servlet-class>restauranttiming.itemadd</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>menu</servlet-name>
<url-pattern>/menu</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>halfmenu</servlet-name>
<servlet-class>restauranttiming.halfmenu</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>halfmenu</servlet-name>
<url-pattern>/halfmenu</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>forgot</servlet-name>
<servlet-class>restauranttiming.forgot</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>forgot</servlet-name>
<url-pattern>/forgot</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>update</servlet-name>
<servlet-class>restauranttiming.itemupdate</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>update</servlet-name>
<url-pattern>/update</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>delete</servlet-name>
<servlet-class>restauranttiming.itemdelete</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>delete</servlet-name>
<url-pattern>/delete</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>Datasource</servlet-name>
<servlet-class>restauranttiming.Datasource</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>Datasource</servlet-name>
<url-pattern>/Datasource</url-pattern>
</servlet-mapping>
</web-app>